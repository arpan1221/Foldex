{
  "name": "foldex",
  "version": "0.1.0",
  "description": "Local-first multimodal RAG system for Google Drive folders",
  "private": true,
  "workspaces": [
    "frontend"
  ],
  "scripts": {
    "dev:backend": "cd backend && uvicorn app.main:app --reload --host 0.0.0.0 --port 8000",
    "dev:frontend": "cd frontend && npm run dev",
    "test:backend": "cd backend && pytest",
    "test:frontend": "cd frontend && npm test",
    "setup": "./scripts/setup.sh",
    "download-models": "python scripts/download_models.py"
  },
  "keywords": [
    "rag",
    "multimodal",
    "local-ai",
    "google-drive"
  ],
  "author": "",
  "license": "MIT"
}

